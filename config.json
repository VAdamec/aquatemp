{
  "name": "Warmlink Add-On",
  "version": "1.0.0",
  "slug": "ha-warmlink-addon",
  "description": "Set of script which, via MQTT, allow Home Assistant to control and show metrics from Warmlink heatpumps",
  "url": "https://github.com/VAdamec/aquatemp",
  "arch": ["amd64", "i386"],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "options": {
    "username": "user@user.com",
    "password": "123456",
    "cloudurl": "https://cloud.linked-go.com:449/crmservice/api",
    "mqttuser": "none",
    "mqttpass": "none",
    "mqttserver": "192.168.1.1",
    "mqttport": "1883",
    "hassname": "my_heatpump",
    "pollintv": "20"
  },
  "schema": {
    "username": "str",
    "password": "str",
    "cloudurl": "str",
    "mqttuser": "str",
    "mqttpass": "str",
    "mqttserver": "str",
    "mqttport": "int",
    "hassname": "str",
    "pollintv": "int"
  }
}